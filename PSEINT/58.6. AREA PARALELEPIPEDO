//ELABORA UN PROGRAMA DONDE CALCULES EL AREA TOTAL DE UN PARALELEPIPEDO

Proceso PARALELEPIPEDO
	
	DEFINIR A, B, C, C1, C2, C3, AREA COMO REALES; 
	
	ESCRIBIR "INGRESE LA BASE"; 
	LEER B; 
	
	ESCRIBIR "INGRESE ALTURA"; 
	LEER A; 
	
	ESCRIBIR "INGRESE ANCHO"; 
	LEER C; 
	
	C1<-(A*B)*(2);
	C2<-(A*C)*(2);
	C3<-(B*C)*(2);
	
	AREA<-C1+C2+C3;
	
	ESCRIBIR "EL AREA DE TU PARALELEPIPEDO ES: ", AREA; 
	
FinProceso
