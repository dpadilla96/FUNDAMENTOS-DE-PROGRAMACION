//DIANA PADILLA ZAMORA
Proceso AreaDeUnParalelepipedo
	Definir largo Como Real;
	Escribir 'favor de ingresar largo';
	Leer largo;
	Definir ancho Como Real;
	Escribir 'favor de ingresar ancho:';
	Leer ancho;
	Definir alto Como Real;
	Escribir 'favor de ingresar la altura:';
	Leer alto;
	Escribir 'El area del paralelepipedo es :',2*((largo*ancho)+(largo*alto)+(ancho*alto));
FinProceso

